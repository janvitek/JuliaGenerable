  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "App",
      "request": "launch",
      "mainClass": "prlprg.App",
      "projectName": "JuliaGenerable2"
    },
    {
      "type": "java",
      "name": "Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "App",
      "request": "launch",
      "mainClass": "prlprg.App",
      "projectName": "JuliaGenerable",
      "cwd": "${workspaceFolder}/"
    },
    {
      "type": "java",
      "name": "Lexer",
      "request": "launch",
      "mainClass": "prlprg.Lexer",
      "projectName": "JuliaGenerable",
      "cwd": "${workspaceFolder}/path/to/your/working/directory"
    },
    {
      "type": "java",
      "name": "Parser",
      "request": "launch",
      "mainClass": "prlprg.Parser",
      "projectName": "JuliaGenerable"
    }
  ]
}